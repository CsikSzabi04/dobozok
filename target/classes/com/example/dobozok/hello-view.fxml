<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox spacing="10.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.dobozok.HelloController">
   <children>
      <Pane fx:id="pnPane" prefHeight="660.0" prefWidth="980.0" style="-fx-background-color: white; -fx-border-color: gray;" />
      <VBox alignment="CENTER" spacing="10.0">
         <children>
            <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@boxopen.png" />
               </image></ImageView>
            <Label fx:id="lbOpendb" text="0 db">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Label>
            <ImageView fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@box.png" />
               </image></ImageView>
            <Label fx:id="lbClosedb" text="0 db">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font></Label>
            <ImageView fx:id="ivArrow" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@arrow.png" />
               </image>
            </ImageView>
            <Label maxHeight="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
            <Button fx:id="btRandom" mnemonicParsing="false" text="Random" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</HBox>
